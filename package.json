{
  "name": "reactextension",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "localforage": "^1.7.1",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4",
    "typeface-asap": "0.0.54"
  },
  "scripts": {
    "start": "react-scripts start",
    "electron-dev": "concurrently \"BROWSER=none npm start\" \"wait-on http://localhost:3000 && electron .\"",
