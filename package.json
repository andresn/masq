    "standard-fix": "standard --fix \"src/**/*.js\""
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "devDependencies": {
    "concurrently": "^3.5.1",
    "electron": "^1.8.4",
    "react-test-renderer": "^16.3.1",
