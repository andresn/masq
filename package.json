{
  "name": "masq-app",
  "description": "masq-app by Qwant",
  "author": "Qwant",
  "license": "MIT",
  "version": "0.1.0",
  "private": true,
  "main": "public/electron.js",
  "homepage": "./",
  "dependencies": {
    "localforage": "^1.7.1",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4",
    "typeface-asap": "0.0.54"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "electron-dev": "ENV=development concurrently \"BROWSER=none npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom --testPathIgnorePatterns=\"<rootDir>/src/masq\"",
    "eject": "react-scripts eject",
    "standard": "standard \"src/**/*.js\"",
    "standard-fix": "standard --fix \"src/**/*.js\"",
    "pack": "npm run build && electron-builder --dir",
    "dist": "npm run build && electron-builder",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "standard": {
    "env": [
      "jest"
    ],
    "ignore": [
      "src/masq/"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.1",
    "@storybook/addon-info": "^3.4.1",
    "@storybook/addon-links": "^3.4.1",
    "@storybook/addon-storyshots": "^3.4.1",
    "@storybook/addons": "^3.4.1",
    "@storybook/react": "^3.4.1",
    "concurrently": "^3.5.1",
    "electron": "^1.8.4",
    "electron-builder": "^20.8.1",
    "react-app-rewired": "^1.5.2",
    "react-test-renderer": "^16.3.1",
    "standard": "^11.0.1",
    "wait-on": "^2.1.0"
  },
  "build": {
    "appId": "masq-app"
  }
}
